<Window x:Class="Async_RestAPI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Async_RestAPI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Label Content="Async project" FontSize="20"></Label>
        <Button Content="Load Sync" Grid.Row="1" FontSize="25" Click="ButtonSyncClick"></Button>
        <Button Content="Load Async" Grid.Row="2" FontSize="25" Click="ButtonAsyncClick"></Button>
        <TextBlock Grid.Row="3" Name="out_txt_block"></TextBlock>
    </Grid>
</Window>
